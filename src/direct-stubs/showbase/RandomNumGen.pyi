__all__ = ['RandomNumGen', 'randHash']

from collections.abc import MutableSequence, Sequence
from typing import Any, ClassVar, SupportsIndex, SupportsInt, TypeVar

from direct.directnotify.Notifier import Notifier

_T = TypeVar('_T')

def randHash(num: SupportsInt | RandomNumGen) -> int: ...

class RandomNumGen:
    notify: ClassVar[Notifier]
    def __init__(self, seed: SupportsInt | RandomNumGen) -> None: ...
    def choice(self, seq: Sequence[_T]) -> _T: ...
    def shuffle(self, x: MutableSequence[Any]) -> None: ...
    def randrange(self, start: SupportsIndex, stop: SupportsIndex | None = None, step: SupportsIndex = 1) -> int: ...
    def randint(self, a: int, b: int) -> int: ...
    def random(self) -> float: ...
